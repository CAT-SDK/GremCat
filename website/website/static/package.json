{
  "name": "website-static",
  "version": "1.0.0",
  "description": "static assets for ideas-uo's django website",
  "main": "webpack.config.js",
  "scripts": {
    "compile:sass": "sass src\\sass\\main.scss dist\\css\\style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' dist\\css\\style.comp.css -o  dist\\css\\style.prefix.css",
    "compress:css": "sass  dist\\css\\style.prefix.css  dist\\css\\style.css --style=compressed",
    "build:js": "webpack",
    "build": "npm-run-all compile:sass prefix:css compress:css build:js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bootstrap": "^5.1.3",
    "jquery": "^3.6.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.7",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "postcss-cli": "^9.1.0",
    "sass": "^1.52.1",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2"
  }
}
